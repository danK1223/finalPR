<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#836382"
    tools:context=".AdminPageEdit">

    <EditText
        android:id="@+id/editGameName"
        android:layout_width="213dp"
        android:layout_height="83dp"
        android:ems="10"
        android:hint="New Name"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:fontFamily="@font/new_font"
        app:layout_constraintVertical_bias="0.172" />

    <EditText
        android:id="@+id/editDescription"
        android:layout_width="207dp"
        android:layout_height="83dp"
        android:ems="10"
        android:hint="New Description"
        android:inputType="textPersonName"
        android:fontFamily="@font/new_font"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.327" />

    <EditText
        android:id="@+id/AddWebsite"
        android:layout_width="200dp"
        android:layout_height="57dp"
        android:ems="10"
        android:hint="New Website"
        android:fontFamily="@font/new_font"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.109"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.459" />

    <EditText
        android:id="@+id/addWebsiteDisplayName"
        android:layout_width="197dp"
        android:layout_height="51dp"
        android:ems="10"
        android:fontFamily="@font/new_font"
        android:hint="New Website Display"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.537" />

    <EditText
        android:id="@+id/editImageURL"
        android:layout_width="207dp"
        android:layout_height="83dp"
        android:ems="10"
        android:fontFamily="@font/new_font"
        android:hint="New Website Image"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.714" />

    <Button
        android:id="@+id/applyNameChange"
        android:layout_width="131dp"
        android:layout_height="40dp"
        android:text="Save"
        android:fontFamily="@font/new_font"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.892"
        app:layout_constraintStart_toStartOf="parent"
        app:cornerRadius="20dp"
        android:textColor="@android:color/black"
        android:backgroundTint="#CE9351"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.186" />

    <Button
        android:id="@+id/applyDescriptionChange"
        android:layout_width="131dp"
        android:layout_height="40dp"
        android:text="Save"
        android:fontFamily="@font/new_font"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.892"
        android:textColor="@android:color/black"
        android:backgroundTint="#CE9351"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.334" />

    <Button
        android:id="@+id/applyAddWebsite"
        android:layout_width="131dp"
        android:layout_height="40dp"
        android:text="Add"
        android:fontFamily="@font/new_font"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.867"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="@android:color/black"
        android:backgroundTint="#CE9351"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <Button
        android:id="@+id/applyEditImageURL"
        android:layout_width="131dp"
        android:layout_height="40dp"
        android:text="Save"
        app:cornerRadius="20dp"
        android:fontFamily="@font/new_font"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.817"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textColor="@android:color/black"
        android:backgroundTint="#CE9351"
        app:layout_constraintVertical_bias="0.69" />

    <Button
        android:id="@+id/goBackBtn"
        android:layout_width="137dp"
        android:layout_height="73dp"
        android:text="Back"
        android:textColor="@android:color/black"
        android:backgroundTint="#CE9351"
        android:fontFamily="@font/new_font"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.942" />

</androidx.constraintlayout.widget.ConstraintLayout>