<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#836382"
    tools:context=".ListTable"
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="-77dp">


    <Button
        android:id="@+id/back1"
        android:layout_width="179dp"
        android:layout_height="53dp"
        android:fontFamily="@font/new_font"
        android:rotationX="-2"
        android:text="go back"
        android:textColor="@android:color/black"
        android:backgroundTint="#CE9351"
        app:cornerRadius="20dp"
        android:textSize="50px"
        app:icon="?attr/homeAsUpIndicator"
        app:iconTint="#2CDFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="193dp"
        android:layout_height="21dp"
        android:text="Select Game:"
        android:fontFamily="@font/new_font"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022"
        tools:text="Select Game:" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="243dp"
        android:layout_height="50dp"
        android:text="Websites list is empty"
        android:textColor="#683434"
        android:textSize="45px"
        android:fontFamily="@font/new_font"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.76"
        tools:visibility="visible" />

    <Button
        android:id="@+id/gotosite"
        android:layout_width="177dp"
        android:layout_height="63dp"
        android:fontFamily="@font/new_font"
        android:text="View the game"
        android:textColor="@android:color/black"
        android:backgroundTint="#CE9351"
        app:cornerRadius="20dp"
        android:textSize="50px"
        app:icon="@android:drawable/ic_menu_view"
        app:iconTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.918"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.872" />

    <Button
        android:id="@+id/editsite"
        android:layout_width="176dp"
        android:layout_height="59dp"
        android:backgroundTint="#CE9351"
        android:fontFamily="@font/new_font"
        android:text="Delete the game"
        android:textColor="@android:color/black"
        android:textSize="50px"
        app:cornerRadius="20dp"
        app:icon="@android:drawable/ic_menu_delete"
        app:iconTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.873"
        tools:visibility="invisible" />

    <Button
        android:id="@+id/editsite3"
        android:layout_width="176dp"
        android:layout_height="59dp"
        android:backgroundTint="#CE9351"
        android:fontFamily="@font/new_font"
        android:text="Edit the game"
        android:textColor="@android:color/black"
        android:textSize="50px"
        app:cornerRadius="20dp"
        app:icon="@android:drawable/ic_menu_delete"
        app:iconTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976"
        tools:visibility="invisible" />

    <ListView
        android:id="@+id/lv"
        android:layout_width="387dp"
        android:layout_height="442dp"
        android:fontFamily="@font/new_font"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.208"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.179" />


</androidx.constraintlayout.widget.ConstraintLayout>

